{
  "siteMetadata": {
    "language": "en",
    "siteUrl": "https://www.chris-young.dev",
    "title": "Chris Young",
    "titleTemplate": "Chris Young personal website and portfolio.",
    "description": "This is the personal website of Chris Young and displays some of his current and past project, blogs, and interests.",
    "author": "Chris Young",
    "avatar": "../content/images/gatsby.png",
    "bio": "This biography will be used in the footer area of an article and can contain some more details about the author.",
    "social": {
      "behance": "https://behance.com",
      "github": "https://github.com/cyoung43",
      "medium": "https://medium.com/@cyoung.byu",
      "linkedin": "https://www.linkedin.com/in/chris-young22/",
      "twitter": "https://twitter.com/chrisyoung0"
    }
  },
  "siteConfiguration": {
    "logo": { "text": "Portfolio" },
    "navigation": {
      "header": [
        { "label": "About", "url": "/#about" },
        { "label": "Blog", "url": "/blog" },
        { "label": "Features", "url": "/#features" },
        { "label": "Github", "url": "/#github" }
      ],
      "ctaButton": {
        "openNewTab": true,
        "label": "Resume",
        "url": "/resume.pdf"
      },
      "footer": [
        { "label": "Privacy", "url": "/privacy" },
        { "label": "Imprint", "url": "/imprint" }
      ]
    },
    "featureToggles": {
      "useDarkModeAsDefault": false,
      "useDarkModeBasedOnUsersPreference": true,
      "useCookieBar": false
    }
  }
}
